{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": { },
    "rules": {
      "arrow-parens": false,
      "interface-name": false,
      "max-classes-per-file": false,
      "member-access": false,
      "no-namespace": false,
      "no-reference": false,
      "no-var-requires": false,
      "ordered-imports": false,
      "quotemark": false,
      "variable-name": [true, "check-format", "allow-leading-underscore", "allow-pascal-case"]
    },
    "rulesDirectory": []
}
